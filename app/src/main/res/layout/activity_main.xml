<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/colorPrimary"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/fragment_container"

            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="100dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </FrameLayout>

        <com.example.inicio_3.CurvedBottomNavigationView
            android:id="@+id/bottom_nav"
            android:layout_width="match_parent"
            android:layout_height="72dp"
            android:background="@color/colorAccent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintEnd_toStartOf="parent">

        </com.example.inicio_3.CurvedBottomNavigationView>

    </androidx.constraintlayout.widget.ConstraintLayout>

        <RelativeLayout
            android:id="@+id/lin_id"
            android:layout_gravity="bottom|center"
            android:layout_margin="40dp"
            android:background="@drawable/oval"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            tools:ignore="MissingConstraints">
            <com.sdsmdg.harjot.vectormaster.VectorMasterView
                android:id="@+id/fab"
                android:layout_centerInParent="true"
                android:layout_margin="12dp"
                android:visibility="visible"
                android:elevation="6dp"
                app:vector_src="@drawable/ic_home_white_24dp"
                android:layout_width="45dp"
                android:layout_height="45dp"/>

            <com.sdsmdg.harjot.vectormaster.VectorMasterView
                android:id="@+id/fab1"
                android:layout_centerInParent="true"
                android:layout_margin="12dp"
                android:visibility="gone"
                android:elevation="6dp"
                app:vector_src="@drawable/ic_pool_black_24dp"
                android:layout_width="45dp"
                android:layout_height="45dp"/>

            <com.sdsmdg.harjot.vectormaster.VectorMasterView
                android:id="@+id/fab2"
                android:layout_centerInParent="true"
                android:layout_margin="12dp"
                android:visibility="gone"
                android:elevation="6dp"
                app:vector_src="@drawable/ic_perm_contact_calendar_white_24dp"
                android:layout_width="45dp"
                android:layout_height="45dp"/>


        </RelativeLayout>




</androidx.coordinatorlayout.widget.CoordinatorLayout>